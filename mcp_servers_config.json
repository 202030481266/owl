{
  "mcpServers": {
    "fetch": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_fetch"
      ]
    },
    "arxiv-mcp-server": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "arxiv-mcp-server",
        "--storage-path",
        "D:\\项目\\owl\\papers\\data"
      ]
    },
    "ddg-search": {
      "command": "uvx",
      "args": [
        "duckduckgo-mcp-server"
      ]
    },
    "g-search": {
      "command": "npx",
      "args": ["-y", "g-search-mcp"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "D:\\"
      ]
    }
  }
}